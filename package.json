{
  "name": "html-elements",
  "description": "Array of all standard HTML and HTML5 elements.",
  "version": "1.0.0",
  "homepage": "https://github.com/jonschlinkert/html-elements",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/html-elements",
  "bugs": {
    "url": "https://github.com/jonschlinkert/html-elements/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha",
    "update": "node support.js"
  },
  "dependencies": {
    "micromatch": "^3.1.4"
  },
  "devDependencies": {
    "delete": "^1.1.0",
    "gh-clone": "^1.0.0",
    "gulp-format-md": "^1.0.0",
    "mocha": "^4.0.1",
    "write-json": "^2.0.0"
  },
  "keywords": [
    "elements",
    "html"
  ],
  "lintDeps": {
    "devDependencies": {
      "files": {
        "patterns": [
          "support.js"
        ]
      }
    }
  },
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "breakdance",
        "html-toc",
        "html-tag",
        "remarkable"
      ]
    },
    "lint": {
      "reflinks": true
    }
  }
}
